<?xml version="1.0" encoding="UTF-8"?>
    <addon id="repository.AramGhamoyan" name="Aram Ghamoyan Repository" version="1.0.7" provider-name="Aram Ghamoyan">
        <requires>
            <import addon="xbmc.addon" version="12.0.0"/>
			<import addon="plugin.video.AwesomeStreams" />
			<import addon="repository.dokinl" />
        </requires>
        <extension point="xbmc.addon.repository" name="Galaxy Repo">
            <info compressed="false">https://raw.githubusercontent.com/aramghamoyan/aramghamoyan/master/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/aramghamoyan/aramghamoyan/master/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/aramghamoyan/aramghamoyan/master/zips</datadir>
            <hashes>false</hashes>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>The Official Aram Ghamoyan repository</summary>
            <description>This repository contains all addons made by Aram Ghamoyan.</description>
            <platform>all</platform>

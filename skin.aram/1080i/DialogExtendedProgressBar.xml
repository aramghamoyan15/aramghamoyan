<?xml version="1.0" encoding="UTF-8"?>
<window>
    <type>dialog</type>
    <controls>
        <control type="group">
            <animation type="WindowOpen" reversible="false">
				<effect type="slide" start="0,-400" end="0,0" tween="cubic" easing="out" time="500" />
			</animation>
			<animation type="WindowClose" reversible="false">
				<effect type="slide" start="0,0" end="0,-400" tween="cubic" easing="out" time="500" delay="1000"/>
			</animation>
            <control type="image" description="Dialog BG">
                <width>1920</width>
                <height>148</height>
				<aspectratio>stretch</aspectratio>
				<texture>$VAR[9000menu]</texture>
            </control>
            <control type="label" id="30" description="Caption Label">
                <top>20</top>
                <left>630</left>
                <width>575</width>
                <height>44</height>
                <font>font35</font>
                <align>left</align>
                <aligny>center</aligny>
                <textcolor>$VAR[notificationtext]</textcolor>
            </control>
            <control type="label" id="31" description="line 1 Label">
                <left>630</left>
                <top>64</top>
                <width>755</width>
                <height>30</height>
                <font>font21</font>
                <align>left</align>
                <textcolor>$VAR[notificationtext]</textcolor>
                <aligny>center</aligny>
            </control>
            <control type="progress" id="32" description="Progress bar">
                <left>622</left>
                <top>95</top>
                <width>678</width>
                <height>15</height>
                <texturebg colordiffuse="Grey4">process/systemsettingsprocessbg.png</texturebg>
				<midtexture colordiffuse="White2">process/systemsettingsprocessM.png</midtexture>
				<righttexture colordiffuse="White2">process/right.png</righttexture>
				<lefttexture colordiffuse="White2">process/left.png</lefttexture>
            </control>
        </control>
    </controls>
</window>
